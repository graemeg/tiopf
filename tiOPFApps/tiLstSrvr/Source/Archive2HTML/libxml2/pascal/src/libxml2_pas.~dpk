package libxml2_pas;

{$R *.res}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS OFF}
{$LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'header translation for gnome libraries libxml2 and libxslt'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

contains
  iconv in 'iconv.pas',
  libxml2 in 'libxml2.pas',
  libxml2_experimental in 'libxml2_experimental.pas',
  libxslt in 'libxslt.pas';

end.
libxslt in 'libxslt.pas';

end.
